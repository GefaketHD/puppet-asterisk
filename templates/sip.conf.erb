;
; SIP Configuration for Asterisk
;

[general]
<% sip_options.sort.map do |name, value|
  if value.is_a? Array
    value.each do |v| -%>
<%= name -%>=<%= v %>
<%  end
  else -%>
<%= name -%>=<%= value %>
<% end
end -%>

#include </etc/asterisk/sip.registry.d/*.conf>
#include </etc/asterisk/sip.d/*.conf>
